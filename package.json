{
  "name": "featurepeek-marketing",
  "private": true,
  "description": "The public website of https://featurepeek.com",
  "version": "0.1.0",
  "author": "Jason Barry <jason@featurepeek.com>",
  "bugs": {
    "url": "https://github.com/featurepeek/marketing/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop --port 3000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint 'src/**/*.{js,jsx}' --cache --ignore-path .gitignore",
    "serve": "gatsby serve --port 3000",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "husky": {
    "hooks": {
      "post-merge": "yarn",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^2.7.1",
    "gatsby-image": "^2.1.1",
    "gatsby-plugin-feed": "^2.2.1",
    "gatsby-plugin-mailchimp": "^5.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-segment-js": "^3.0.1",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-images": "^3.0.12",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-source-stripe": "^2.2.2",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "prismjs": "^1.16.0",
    "rc-switch": "^1.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "ui-box": "^2.0.0"
  },
  "devDependencies": {
    "@featurepeek/circleci": "^0.1.7",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "homepage": "https://github.com/featurepeek/marketing#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/featurepeek/marketing.git"
  }
}
